<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.copying.blogs.mapper.CyBlogsPhotoFileMapper">
    <insert id="insertPhotoFile">
        insert into cy_blogs_photo_file (file_id,name,file_path,file_type,file_class,file_size) value
        (#{fileId},#{name},#{filePath},#{fileType},#{fileClass},#{fileSize});
    </insert>
</mapper>